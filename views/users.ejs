<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('partials/head.ejs', {title: 'Users'}) %>
  </head>
  <body>
    <%- include('partials/navbar.ejs') %>
    <div class="container w-50">
      <h1 class="mt-3 mb-3">All Users</h1>
      <div class="list-group">
        <% for(let u of users){ %>
        <a
          href="/users/<%= u %> "
          class="list-group-item list-group-item-action"
        >
          USER: #<%= u %>
        </a>
        <% } %>
      </div>
    </div>
  </body>
</html>
